game <- function(){
x <- sum(sample(1:6,2, replace = T))
#  print(paste0('the point is ', x))
if (x %in% c(2, 3, 12)){
return("Loss")
} else if(x %in% c(7, 11)){
return("Win")
} else{
x1 = sum(sample(1:6,2, replace = T))
#    print(paste0('the second roll is ', x1))
while (x1 != 7 | x1 != x){
if(x1 == 7){
return("Loss")
} else if(x1 == x){
return("Win")
} else{
x1 = sum(sample(1:6,2, replace = T))
#        print(paste0('the next roll is ', x1))
}
}
}
}
game()
result = c()
for (i in c(1:1000)){
result = c(result, game())
}
as.data.frame(table(result))
